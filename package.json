  "repository": "pdfgeneratorapi/pdfviewer",
  "version": "1.3.0",
  "description": "PDF viewer by PDF Generator API",
  "main": "./dist/PDFViewer.cjs",
  "module": "./dist/PDFViewer.mjs",
